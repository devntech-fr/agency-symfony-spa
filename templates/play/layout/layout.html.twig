{% extends "base/base.html.twig" %}

{% block html_class %}scroll-smooth{% endblock %}

{% block meta %}
    {{ parent() }}
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
{% endblock %}

{% block title %}
    Play | Free Tailwind CSS Template for Startup and SaaS By TailGrids
{% endblock %}

{% block favicon %}
    <link
            rel="shortcut icon"
            href="{{ asset('/play/images/favicon.png') }}"
            type="image/x-icon"
    />
{% endblock %}

{% block stylesheets %}
    {{ encore_entry_link_tags('play') }}
{% endblock %}

{% block header %}
    {% include 'play/layout/header.html.twig' %}
{% endblock %}

{% block body %}{% endblock %}

{% block footer %}
    {% include 'play/layout/footer.html.twig' %}
{% endblock %}
{% block back_to_top %}
    {% include 'play/layout/back_to_top.html.twig' %}
{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('play') }}
{% endblock %}
